{
  "name": "okornok",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "angular-cli": {},
  "scripts": {
    "preserve": "npm run lint",
    "lint": "ng lint",
    "serve": "npm run preserve && ng serve --proxy-config proxy.conf.json",
    "start": "pm2 start .config/pm2.process.json && pm2 logs",
    "postinstall": "npm run git-hook",
    "start:dev": "NODE_ENV=development npm start",
    "test": "npm run test:server && npm run test:client",
    "test:server": "npm run migrate && ./node_modules/.bin/mocha ./server/specs --colors --recursive --timeout 15000 --require babel-core/register",
    "test:server:test:db": "npm run migrate:test:db && NODE_ENV=test ./node_modules/.bin/mocha ./server/specs --colors --recursive --timeout 10000 --require babel-core/register",
    "deploy": "bash ./server/bin/deploy-to-heroku.sh",
    "deploy-aws": "bash ./server/bin/deploy-to-amazon.sh",
    "migrate": "bash ./server/bin/run-migrations-n-seeders.sh",
    "migrate:test:db": "NODE_ENV=test bash ./server/bin/run-migrations-n-seeders.sh",
    "docs:server": "./node_modules/.bin/jsdoc --configure .config/conf.jsdoc.server.json",
    "git-hook": "bash ./server/bin/deploy/git-hook.sh",
    "ng": "ng",
    "dev-server:client": "ng serve",
    "build:client": "ng build --env=prod",
    "test:client": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
